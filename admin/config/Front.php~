 <?php
defined('ACCESS') or die('Access Denied');


	$template = Template::getActive();


$auth = new Auth();


//echo $auth->authorized?'true':'false';

if($auth->authorized)
{
	$user = $auth->user;
	$page = new AdminPage(new Template(array('name'=>ADMIN_TEMPLATE)));
}
else
{
	$page = new AdminPage(new Template(array('name'=>'login')));
}


$page->display();
